{
  "presets": [
    "stage-0",
    [
      "env",
      {
        "targets": {
          "node": "4"
        }
      }
    ]
  ],
  "plugins": [
    "transform-flow-comments"
  ],
  "sourceRoot": "src",
  "env": {
    "development": {
      "compact": false,
      "comments": true
    },
    "production": {
      "presets": [
        "flow"
      ],
      "plugins": [
        "remove-comments"
      ],
      "compact": true,
      "comments": false,
      "minified": true
    }
  }
}
